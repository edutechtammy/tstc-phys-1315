    <item ident="2a56a624ecccf4dd31605146c548ba66" title="Action/Reaction Force Pair (iii)">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
